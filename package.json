{
  "name": "mad-minute",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "setup": "npm i -g webpack webpack-dev-server",
    "postsetup": "npm i & webdriver-manager update",
    "dev": "webpack -d -c --display-error-details",
    "watch": "npm run dev -- -w",
    "preprod": "npm i",
    "prod": "NODE_ENV=production webpack -p --config webpack.prod.config.js -c --display-error-details",
    "preserve": "npm run dev",
    "serve": "webpack-dev-server --port 5000 --content-base www/build/ --inline -d -c --display-error-details",
    "pree2e:start": "webdriver-manager update",
    "e2e:start": "parallelshell \"npm start\" \"webdriver-manager start\"",
    "e2e": "protractor protractor.conf.js",
    "start": "npm run serve"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "body-parser": "^1.14.2",
    "clean-webpack-plugin": "^0.1.5",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "express-json": "^1.0.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "http": "0.0.0",
    "lodash": "^3.10.1",
    "node-sass-loader": "^0.1.7",
    "parallelshell": "^2.0.0",
    "request": "^2.67.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "to-string-loader": "^1.1.3",
    "ts-loader": "^0.7.2",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.7.5",
    "urlencoded-request-parser": "^1.0.1",
    "wallaby-webpack": "0.0.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.33.3",
    "lodash": "3.10.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10",
    "webpack": "^1.12.9"
  }
}
